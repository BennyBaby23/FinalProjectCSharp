@model IEnumerable<FinalProject1.Models.ProuctSore>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StoreName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StoreAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StoreReview)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StoreName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StoreAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StoreReview)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ProductStoreID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ProductStoreID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ProductStoreID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
